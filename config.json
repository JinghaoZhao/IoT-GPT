{
  "sensors": [
    {
      "type": "temperature",
      "model": "DHT22",
      "pin": 4
    },
    {
      "type": "humidity",
      "model": "DHT22",
      "pin": 4
    }
  ],
  "networks": [
    {
      "category": "cellular",
      "type": "Cat-M1",
      "apn": "your_apn"
    },
    {
      "category": "wifi",
      "ssid": "your_ssid",
      "password": "your_wifi_password"
    }
  ],
  "traffic_interval": 60,
  "protocol": {
    "type": "MQTT",
    "broker": "localhost",
    "port": 8883,
    "username": "",
    "password": "",
    "topic": "test/topic",
    "security": {
      "encryption": "TLS",
      "ca_certificate_path": "./certs/ca.crt",
      "client_certificate_path": "./certs/client.crt",
      "client_key_path": "./certs/client.key"
    }
  }
}